{
    "name": "myvidster/aro-payments",
    "type": "project",
    "description": "A payments processor for the Arionum cryptocurrency.",
    "require": {
        "php": "^7.1"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "myvidster\\AroPayments\\": "src"
        }
    },
    "scripts": {
        "check-style": "phpcs src",
        "fix-style": "phpcbf src"
    },
    "config": {
        "sort-packages": true
    }
}
